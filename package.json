{
  "name": "my-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- tsx src/lib/db/migrate.ts",
    "db:test-connection": "dotenv -e .env.local -- tsx src/lib/db/test-db-connection.ts",
    "db:drop": "dotenv -e .env.local -- drizzle-kit drop",
    "db:pull": "dotenv -e .env.local -- drizzle-kit introspect",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:check": "dotenv -e .env.local -- drizzle-kit check",
    "kb:index": "dotenv -e .env.local -- tsx src/scripts/kb/index.ts",
    "kb:reset": "dotenv -e .env.local -- tsx src/scripts/kb/reset.ts",
    "kb:test-chunking": "dotenv -e .env.local -- tsx src/scripts/kb/test-chunking.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.18",
    "@ai-sdk/react": "^3.0.66",
    "@heroicons/react": "^2.2.0",
    "@langchain/community": "^1.1.8",
    "@langchain/core": "^1.1.17",
    "@t3-oss/env-nextjs": "^0.13.10",
    "ai": "^6.0.64",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "nanoid": "^5.1.6",
    "next": "^16.1.6",
    "pdf-parse": "^1.1.1",
    "pg": "^8.18.0",
    "postcss": "^8.5.6",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
